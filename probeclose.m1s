If (GetOEMLED(809) Or GetOEMLED(807)) Then	Message "Please Home Machine First"	EndElseEnd IfOutProbeClose = 1Call ProbeClose()Sub ProbeClose	Message "Probe closing..."	Call SetCsmioOutEx(OutProbeClose, True)	Sleep(5200)	Call SetCsmioOutEx(OutProbeClose, False)	Message "Probe Closed"End SubPublic Sub SetCsmioOutEx(ByVal n As Integer, ByVal state As Boolean)	If(state) Then		SetOutBit(100, n)	Else		ResetOutBit(100, n)	End IfEnd Sub