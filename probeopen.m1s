If (GetOEMLED(809) Or GetOEMLED(807)) Then	Message "Please Home Machine First"	EndElseEnd IfOutProbeOpen = 0Call ProbeOpen()Sub ProbeOpen	Message "Probe opening..."	Call SetCsmioOutEx(OutProbeOpen, True)	Sleep(2200)	Call SetCsmioOutEx(OutProbeOpen, False)	Message "Probe Opened."End SubPublic Sub SetCsmioOutEx(ByVal n As Integer, ByVal state As Boolean)	If(state) Then		SetOutBit(100, n)	Else		ResetOutBit(100, n)	End IfEnd Sub