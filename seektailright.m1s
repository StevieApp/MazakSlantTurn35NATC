If (GetOEMLED(809) Or GetOEMLED(807)) Then	Message "Please Home Machine First"	EndElseEnd IfConst OEMButtonStop = 1003Const TailCouple = 1820Const ZPositionLastCouple = 1821Zaxis = 2Xaxis = 0CurrPosZ = GetABSPosition(Zaxis)CurrPosX = GetABSPosition(Xaxis)If  IsActive(INPUT2) Then	Message "TailStock Already Lockable"	EndElse	Message "TailStock Not Lockable."	Sleep(200)	Message "Moving..."End IfCode "G53 G00 X00"While IsMoving()WendCode "G53 G00 Z615 F590"While IsMoving()	If  IsActive(INPUT2) Then		Sleep(110)		DoOEMButton(OEMButtonStop)		Sleep(2000)	Else	End IfWendIf  IsActive(INPUT2) Then	Message "TailStock Lockable"	Sleep(500)	ActivateSignal(OUTPUT4)	SetUserDRO(ZPositionLastCouple, -2000)	SetUserDRO(TailCouple, -1) 	Message "TailStock Coupled"	Code ("G53 G00 Z"& CurrPosZ)	''Code ("G53 G00 Z"& CurrPosZ &"X"&CurrPosX)Else	Message "TailStock Not Found."	Sleep(1000)	Message "Seek Manually then Press Couple"End If