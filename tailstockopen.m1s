If (GetOEMLED(809) Or GetOEMLED(807)) Then	Message "Please Home Machine First"	EndElseEnd IfConst InTailStockOut = 6ActivateSignal(Enable5)Message "Opening Tailstock..."Sleep(50)While Not GetCsmioInEx(InTailStockOut)WendMessage "Tailstock Open"DeactivateSignal(Enable5)Public Function GetCsmioInEx(n As Integer) As Boolean	Dim reg As Integer	If(n<16) Then		reg = 100	Else		reg = 101		n = n-16	End If	If(GetInBit(reg, n)) Then		GetCsmioInEx = true	Else		GetCsmioInEx = False	End If	Exit FunctionEnd Function 